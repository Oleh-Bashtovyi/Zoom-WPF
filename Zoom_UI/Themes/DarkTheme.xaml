<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainBrush" Color="LightBlue"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="#5050B8"/>
    <SolidColorBrush x:Key="InformativeBrush" Color="White"/>
    <SolidColorBrush x:Key="WarningBrush" Color="Goldenrod"/>
    <SolidColorBrush x:Key="OffButton" Color="#D075FF"/>
    <SolidColorBrush x:Key="OnButton" Color="#5050B8"/>


    <SolidColorBrush x:Key="MainFontBrush" Color="White"/>
    <SolidColorBrush x:Key="SecondaryFontBrush" Color="Silver"/>
</ResourceDictionary>