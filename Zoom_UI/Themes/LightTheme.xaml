<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MainBrush" Color="LightYellow"/>
    <SolidColorBrush x:Key="MainFontBrush" Color="Black"/>
    <SolidColorBrush x:Key="CurrentUserBackgroundBrush" Color="#FCD371"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="White"/>
    <SolidColorBrush x:Key="SecondaryFontBrush" Color="#4C4B4B"/>
    <SolidColorBrush x:Key="InformativeBrush" Color="#00E400"/>
    <SolidColorBrush x:Key="WarningBrush" Color="#FF3F23"/>
    <SolidColorBrush x:Key="OffButton" Color="#FF3F23"/>
    <SolidColorBrush x:Key="OnButton" Color="White"/>

</ResourceDictionary>